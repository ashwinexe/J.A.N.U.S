{
    "manifest_version" : 2,
    "name": "job_application_tracker",
    "description": "Track job applications seamlessly",
    "version" : "1.0.0",
    "icons": {"128" : "icon_128.png"},
    "browser_action": {
        "default_icon" : "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "tabs"],
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["script.js"],
          "run_at": "document_end"
        }
      ]      
}